<span class="button api vote good" no="{{ p.id.value }}" url="/post/api/?call=vote&mode=good&id={{ p.id.value }}" selector=".api.vote.good[no='{{ p.id.value }}'] .text">
	<span class="text">{{ p.vote_good.value }}</span>
    {{ ko ? '찬성' : 'LIKE' }}{{ ko ? '' : p.vote_good.value > 1 ? 'S' }}
</span>
<span class="button api vote bad" no="{{ p.id.value }}" url="/post/api/?call=vote&mode=bad&id={{ p.id.value }}" selector=".api.vote.bad[no='{{ p.id.value }}'] .text">
	<span class="text">{{ p.vote_bad.value }}</span>
    {{ ko ? '반대' : 'DISLIKE' }}{{ ko ? '' : p.vote_bad.value > 1 ? 'S' }}
</span>
